   "source": [
    "def derivative_x(f, x, step_size):\n",
    "    m = (f(x + step_size) - f(x)) / ((x + step_size) - x)\n",
    "    return m\n",
    "\n",
    "\n",
    "def my_function(x):\n",
    "    return x**2\n",
    "\n",
    "slope_at_2 = derivative_x(my_function, 2, .00001)\n",
    "\n",
    "print(slope_at_2)"
   ]
